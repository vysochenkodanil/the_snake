def main(a, b, c):
    new_list = []
    for i in range(c):
        new_list.append(a[i])
        new_list.append(b[i])
    
    return new_list    

if __name__ == '__main__':
    c = int(input())
    a = list(map(int, input().split()))
    b = list(map(int, input().split()))
    
    result = main(a, b, c)
    print(' '.join(map(str, result)))
#чуть голову не сломал с вашими алгоритмами
